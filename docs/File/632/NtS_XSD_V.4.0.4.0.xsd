				</xs:annotation>
			</xs:element>
			<xs:element name="difference" type="nts:difference_type" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Difference with comparative value</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
